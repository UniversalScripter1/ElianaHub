local function onIntroFinished() loadstring(game:HttpGet("https://raw.githubusercontent.com/UniversalScripter1/ElianaHub/refs/heads/main/MM2"))() end local function elianaIntroV10() local p = game:GetService("Players").LocalPlayer local pg = p:WaitForChild("PlayerGui") local l = game:GetService("Lighting") local sg = Instance.new("ScreenGui") sg.Name = "ElianaHubIntro_V10" sg.Parent = pg sg.IgnoreGuiInset = true local b = Instance.new("BlurEffect") b.Size = 0 b.Parent = l local bg = Instance.new("Frame") bg.Size = UDim2.new(1, 0, 1, 0) bg.BackgroundColor3 = Color3.fromRGB(0, 0, 0) bg.BackgroundTransparency = 1 bg.BorderSizePixel = 0 bg.Parent = sg local function cl(c, t, z, s) local lbl = Instance.new("TextLabel") lbl.Size = UDim2.new(1, 0, 0.2, 0) lbl.Position = UDim2.new(0, 0, 0.4, 0) lbl.BackgroundTransparency = 1 lbl.Font = Enum.Font.GothamBold lbl.TextSize = 100 lbl.TextColor3 = c lbl.Text = "ELIANA HUB" lbl.TextTransparency = 1 lbl.ZIndex = z lbl.Parent = sg if not s then local g = Instance.new("UIGradient") g.Color = ColorSequence.new{ColorSequenceKeypoint.new(0, c), ColorSequenceKeypoint.new(0.4, c), ColorSequenceKeypoint.new(0.7, Color3.fromRGB(160, 32, 240)), ColorSequenceKeypoint.new(1, Color3.fromRGB(110, 0, 180))} g.Rotation = 90 g.Parent = lbl end return lbl end local sh = cl(Color3.fromRGB(0, 0, 0), 0.3, 1, true) local g1 = cl(Color3.fromRGB(160, 32, 240), 0.8, 2) local m = cl(Color3.fromRGB(255, 255, 255), 0, 3) local r = cl(Color3.fromRGB(255, 50, 50), 0.5, 4) local bl = cl(Color3.fromRGB(50, 50, 255), 0.5, 4) r.Visible = false bl.Visible = false local function gb() local br = Instance.new("Frame") br.BackgroundColor3 = (math.random() > 0.5 and Color3.fromRGB(160, 32, 240) or Color3.fromRGB(255, 255, 255)) br.BorderSizePixel = 0 br.Position = UDim2.new(0, 0, math.random(0, 100)/100, 0) br.Size = UDim2.new(1, 0, 0, math.random(1, 5)) br.Parent = sg task.delay(0.05, function() br:Destroy() end) end task.spawn(function() for i = 0, 15 do b.Size = i bg.BackgroundTransparency = 1 - (i/30) task.wait(0.01) end m.TextTransparency = 0 sh.TextTransparency = 0.3 g1.TextTransparency = 0.8 for i = 1, 35 do local x = math.random(-25, 25) local y = math.random(-5, 5) local cs = math.random(95, 115) m.TextSize = cs sh.TextSize = cs g1.TextSize = cs if i % 2 == 0 then gb() end m.Position = UDim2.new(0, x/3, 0.4, y/3) sh.Position = UDim2.new(0, (x/3) + 4, 0.4, (y/3) + 4) g1.Position = UDim2.new(0, (x/3) + 2, 0.4, (y/3) + 2) r.Visible = true r.Position = UDim2.new(0, x, 0.4, y) bl.Visible = true bl.Position = UDim2.new(0, -x, 0.4, -y) task.wait(0.03) end r:Destroy() bl:Destroy() m.TextSize = 100 sh.TextSize = 100 g1.TextSize = 100 m.Position = UDim2.new(0, 0, 0.4, 0) sh.Position = UDim2.new(0, 4, 0.4, 4) task.wait(2) for i = 0, 1, 0.1 do local inv = 1 - i m.TextTransparency = i sh.TextTransparency = 0.3 + (i * 0.7) g1.TextTransparency = 0.8 + (i * 0.2) b.Size = 15 * inv bg.BackgroundTransparency = 0.5 + (i * 0.5) task.wait(0.03) end b:Destroy() sg:Destroy() onIntroFinished() end) end elianaIntroV10()

loadstring(game:HttpGet("https://pastebin.com/raw/St5JT0Kf"))()
